<section>
    <div class="container">
       <div class=" mt-3 mb-3" style="float: right;">
            <a href="/admin/add-product" class="btn btn-success"> Add prodects</a>

      </div>
      
       
        <table class="table table-bordered">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Title</th>
      <th scope="col">Category</th>
      <th scope="col">Description</th>
      <th>Image</th>
    </tr>
  </thead>
  <tbody>
    {{#each prodects}}
    <tr>
      <th scope="row">1</th>
      <td>{{this.name}}</td>
      <td>{{this.category}}</td>
      <td>{{this.description}}</td>
      <td><img  style="width: 50px;height: 50%; " src="{{this.Image}}" alt=""></td>
    </tr>
    {{/each}}
    
  </tbody>
</table>


    </div>



</section>